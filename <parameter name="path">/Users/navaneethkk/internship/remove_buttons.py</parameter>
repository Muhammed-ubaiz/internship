<parameter name="content">#!/usr/bin/env python3
import re

with open('frond-end/src/pages/studentpage/Studentsdashboard.jsx', 'r') as f:
    content = f.read()

# Remove the duplicate buttons section - match from comment to closing divs
pattern = r'/\*\s*PUNCH IN/OUT BUTTONS\s*-\s*AT BOTTOM\s*\*/.*?</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>\s*</div>'
content = re.sub(pattern, '', content, flags=re.DOTALL)

with open('frond-end/src/pages/studentpage/Studentsdashboard.jsx', 'w') as f:
    f.write(content)

print('Done')
</parameter>
